{
  "name": "bitbook-api",
  "version": "1.0.0",
  "description": "API para o sistema Bitbook",
  "main": "dist/main.js",
  "scripts": {
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npx nest start",
    "start:local": "npx nest start --watch",
    "start:development": "node dist/main",
    "start:production": "node dist/main",
    "start:debug": "npx nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:local": "docker compose -f docker-compose.dev.yml --env-file .env.local up --build",
    "docker:down": "docker compose down",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "docker exec bitbook-api npx typeorm-ts-node-commonjs migration:create",
    "migration:generate": "docker exec bitbook-api npx typeorm-ts-node-commonjs migration:generate -d src/core/database/typeorm.config.ts",
    "migration:run": "docker exec bitbook-api npx typeorm-ts-node-commonjs migration:run -d src/core/database/typeorm.config.ts",
    "migration:revert": "docker exec bitbook-api npx typeorm-ts-node-commonjs migration:revert -d src/core/database/typeorm.config.ts",
    "migration:show": "docker exec bitbook-api npx typeorm-ts-node-commonjs migration:show -d src/core/database/typeorm.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@aws-sdk/client-ses": "^3.665.0",
    "@aws-sdk/s3-request-presigner": "^3.817.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.6.0",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "sharp": "^0.34.2",
    "typeorm": "^0.3.17",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
